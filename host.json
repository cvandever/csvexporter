{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": false,
                "excludedTypes": "Request"
            }
        },
        "logLevel": {
            "default": "Information",
            "Function": "Information",
            "Host.Results": "Information",
            "Host.Aggregator": "Information"
        }
    },
    "extensionBundle": {
        "id": "Microsoft.Azure.Functions.ExtensionBundle",
        "version": "[4.*, 5.0.0)"
    },
    "extensions": {
        "blobs": {
            "maxDegreeOfParallelism": 4
        }
    }
}